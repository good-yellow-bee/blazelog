# BlazeLog Server Environment Variables
# Copy this file to server.env and fill in the values

# Required - Generate with: openssl rand -base64 32
BLAZELOG_MASTER_KEY=

# Required - Generate with: openssl rand -base64 32
BLAZELOG_JWT_SECRET=

# Optional (for Web UI CSRF protection) - Generate with: openssl rand -base64 32
BLAZELOG_CSRF_SECRET=

# ClickHouse password (required for prod profile)
CLICKHOUSE_PASSWORD=

# Build info (auto-set by CI)
VERSION=dev
COMMIT=
BUILD_TIME=
